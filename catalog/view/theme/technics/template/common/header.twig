<!doctype html>
<html dir="{{ direction }}" lang="{{ lang }}">
  <head>
    <meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="format-detection" content="telephone=no">
	<meta name="MobileOptimized" content="380">
    <base href="{{ base }}">
    <title>{{ title }}</title>
    <meta name="theme-color" content="#{% if (theme_color) %}{{ theme_color_2 }}{% else %}3660BC{% endif %}">
    <meta name="format-detection" content="telephone=no">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
{% if robots %}
		<meta name="robots" content="{{ robots }}" />
{% endif %}
{% if (description) %} 
		<meta name="description" content="{{ description }}">
{% endif %} 
{% if (keywords) %} 
		<meta name="keywords" content= "{{ keywords }}">
{% endif %} 
{% if (google_site_verification) %} 
		<meta name="google-site-verification" content="{{ google_site_verification }}">
{% endif %} 
{% if (yandex_verification) %} 
		<meta name="yandex-verification" content="{{ yandex_verification }}">
{% endif %} 
{% if (fav_180) %} 
		<link rel="apple-touch-icon" sizes="180x180" href="{{ fav_180 }}">
{% endif %} 
{% if (fav_32) %} 
		<link rel="icon" type="image/png" href="{{ fav_32 }}" sizes="32x32">
{% endif %} 
{% if (fav_16) %} 
		<link rel="icon" type="image/png" href="{{ fav_16 }}" sizes="16x16">
{% endif %} 
{% if (open_graph) %} 
		<meta property="og:title" content="{{ title }} {% if (_GET['page'] is defined) %} {{ " - "~ _GET['page']~" "~text_page }}{% endif %}" />
		<meta property="og:type" content="website">
		<meta property="og:url" content="{{ og_url }}">
{% if (og_image) %} 
		<meta property="og:image" content="{{ og_image }}">
{% else %} 
		<meta property="og:image" content="{{ logo }}">
{% endif %} 
		<meta property="og:site_name" content="{{ name }}">
{% endif %} 

{% if (fontawesome == 1) %} 
	<link rel="stylesheet" href="catalog/view/theme/technics/css/fontawesome/svg-with-js.min.css{{ '?v'~version }}">
{% elseif (fontawesome == 2) %}
	<link rel="stylesheet" href="catalog/view/theme/technics/css/fontawesome/4.7.0/css/font-awesome.min.css{{ '?v'~version }}">
{% endif %}
	<link rel="stylesheet" href="catalog/view/theme/technics/css/bootstrap/{{ bootstrap_ver ? '4.4.1' : '3.3.5' }}/bootstrap.min.css{{ '?v'~version }}">
	<link rel="stylesheet" href="catalog/view/theme/technics/css/app.min.css{{ '?v'~version }}">
